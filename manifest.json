{
	"manifest_version": 3,
	"name": "vitaHR Модуль",
	"version": "7.0.8",
	"description": "Магическая кнопка, которая упростит вашу работу",

	"icons": {
		"16": "pict/logo_16.png",
		"32": "pict/logo_32.png",
		"48": "pict/logo_48.png",
		"128": "pict/logo.png"
	},

	"permissions": [ 
		"tabs", 
		"nativeMessaging",
		"storage"
	],
	"optional_host_permissions" : [
		"http://*/*", "https://*/*" 
	],

	"background": {
		"service_worker": "worker.js"
	},

	"action": {
		"default_icon": {
			"16": "pict/logo_16.png",
			"32": "pict/logo_32.png",
			"48": "pict/logo_48.png",
			"128": "pict/logo.png"
		},
		"default_title": "Импорт резюме в Service Desk",
		"default_popup": "popup/load.html"
	},
	"content_scripts": [
		{
		  "matches": [ "<all_urls>" ],
		  "js": ["jquery-3.6.0.js", "utils.js"]
		}
	  ],

	"options_page": "settings.html"

}
